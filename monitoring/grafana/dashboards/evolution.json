{
  "uid": "alpha-evolve-evolution",
  "title": "Alpha-Evolve Evolution",
  "description": "Real-time strategy evolution monitoring (spec-010)",
  "tags": [
    "alpha-evolve",
    "evolution",
    "strategy"
  ],
  "timezone": "browser",
  "refresh": "5s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "experiment",
        "type": "query",
        "datasource": "QuestDB",
        "query": "SELECT DISTINCT experiment FROM evolution_metrics WHERE timestamp > now() - interval '7d'",
        "refresh": 1,
        "multi": false,
        "includeAll": false,
        "current": {}
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "Fitness Progress",
      "description": "Best fitness (Calmar ratio) over time with trend line",
      "type": "timeseries",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 12,
        "h": 8
      },
      "datasource": "QuestDB",
      "targets": [
        {
          "refId": "A",
          "rawSql": "SELECT timestamp, max(calmar) as best_calmar FROM evolution_metrics WHERE experiment = '${experiment}' AND mutation_outcome = 'success' AND timestamp > now() - interval '7d' SAMPLE BY 5m",
          "format": "time_series",
          "datasource": {
            "type": "questdb-questdb-datasource",
            "uid": "P2596F1C8E12435D2"
          }
        },
        {
          "refId": "B",
          "rawSql": "SELECT timestamp, max(sharpe) as best_sharpe FROM evolution_metrics WHERE experiment = '${experiment}' AND mutation_outcome = 'success' AND timestamp > now() - interval '7d' SAMPLE BY 5m",
          "format": "time_series",
          "hide": true,
          "datasource": {
            "type": "questdb-questdb-datasource",
            "uid": "P2596F1C8E12435D2"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10,
            "gradientMode": "scheme",
            "lineWidth": 2,
            "showPoints": "auto"
          },
          "color": {
            "mode": "palette-classic"
          },
          "unit": "none",
          "decimals": 3
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "best_calmar"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Calmar Ratio"
              },
              {
                "id": "color",
                "value": {
                  "fixedColor": "green",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "best_sharpe"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Sharpe Ratio"
              },
              {
                "id": "color",
                "value": {
                  "fixedColor": "blue",
                  "mode": "fixed"
                }
              }
            ]
          }
        ]
      },
      "options": {
        "legend": {
          "showLegend": true,
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
