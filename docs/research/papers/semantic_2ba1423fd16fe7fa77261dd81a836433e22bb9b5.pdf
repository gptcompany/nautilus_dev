					document.addEventListener("DOMContentLoaded", function() {
				var keywords = [{"text":"sales optimization","size":1},{"text":"key performance indicators (kpis)","size":1},{"text":"optimizationcross-time zone management","size":1},{"text":"follow-up timing","size":1},{"text":"sales compliance governance","size":1},{"text":"policy automation","size":1},{"text":"risk-performance alignment","size":1},{"text":"guizhou folk song","size":1},{"text":"vocal performance","size":1},{"text":"ethnic music","size":1},{"text":"environmental singing","size":1},{"text":"music teaching","size":1},{"text":"music training","size":1},{"text":"piano education","size":1},{"text":"piano teaching","size":1},{"text":"chinese piano pedagogy","size":1},{"text":"teaching methodology evolution","size":1},{"text":"cultural integration","size":1},{"text":"china-belarus comparison","size":1},{"text":"technological transformation","size":1},{"text":"large language models","size":1},{"text":"asset\u2011return prediction","size":1},{"text":"textual\u2011sentiment factor","size":1},{"text":"machine learning","size":1},{"text":"information ratio","size":1},{"text":"interpretability","size":1},{"text":"cfius review mechanisms","size":1},{"text":"cross-border lithium battery investment","size":1},{"text":"national security regulation","size":1},{"text":"investment facilitation framework","size":1},{"text":"chinese-style modernization","size":1},{"text":"the cohesion of the chinese nation","size":1},{"text":"contact","size":1},{"text":"artificial intelligence (ai)","size":1},{"text":"digital transformation","size":1},{"text":"architecture industry","size":1},{"text":"generative design","size":1},{"text":"building information modeling (bim)","size":1},{"text":"computer vision","size":1},{"text":"natural language processing (nlp)","size":1},{"text":"business process optimization","size":1},{"text":"sustainability","size":1},{"text":"risk management","size":1},{"text":"automation","size":1},{"text":"design innovation","size":1},{"text":"artificial intelligence","size":1},{"text":"translation","size":1},{"text":"language education","size":1},{"text":"cultural concept","size":1},{"text":"machine translation","size":1}];
				var totalWeight = 0;
				var width = 300;
				var height = 200;
				var transitionDuration = 200;	
				var length_keywords = keywords.length;
				var layout = d3.layout.cloud();

				layout.size([width, height])
					.words(keywords)
					.fontSize(function(d)
					{
						return fontSize(+d.size);
					})
					.on('end', draw);
				
				var svg = d3.select("#wordcloud").append("svg")
					.attr("viewBox", "0 0 " + width + " " + height)	
					.attr("width", '100%');		
				
				function update() {
					var words = layout.words();
					fontSize = d3.scaleLinear().range([16, 34]);
					if (words.length) {
						fontSize.domain([+words[words.length - 1].size || 1, +words[0].size]);
					}
				}
				
				keywords.forEach(function(item,index){totalWeight += item.size;});

				update();

				function draw(words, bounds) {
					var w = layout.size()[0],
						h = layout.size()[1];

					scaling = bounds
						? Math.min(
							w / Math.abs(bounds[1].x - w / 2),
							w / Math.abs(bounds[0].x - w / 2),
							h / Math.abs(bounds[1].y - h / 2),
							h / Math.abs(bounds[0].y - h / 2),
						) / 2
						: 1;

					svg
					.append("g")
					.attr(
						"transform",
						"translate(" + [w >> 1, h >> 1] + ")scale(" + scaling + ")",
					)
					.selectAll("text")
						.data(words)
					.enter().append("text")
